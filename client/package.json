{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.22.0",
    "axios": "^0.19.0",
    "dotenv": "^8.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-facebook-login": "^4.1.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-dev": "dotenv -e .env.development react-scripts build",
    "build-prod": "node_modules/dotenv/lib/dotenv -e .env.production react-scripts build",
    "build": "react-scripts build && rm -r ../server/build && mv build ../server",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0"
  }
}
